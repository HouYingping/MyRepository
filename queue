#include<iostream>
#include<queue>
#include<string>
#include<algorithm>
using namespace std;
/*
队列容器
单向

*/
void test() {
	queue<int> q;
	for (int i = 0;i < 10;i++) {
		q.push(i);
	}
	cout << "队列的大小为：" << q.size() << endl;
	cout << "队列中的第一个元素：" << q.front() << endl;
	cout << "队列中的最后一个元素：" << q.back() << endl;
	while (!q.empty())
	{
		cout << q.front() << endl;
		q.pop();
	}
}
int main() {
	test();
	system("pause");
	return 0;
}
