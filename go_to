#include <iostream>
using namespace std;
int main() {
	// goto语句
	cout << "1" << endl;
	cout << "2" << endl;
	cout << "3" << endl;
	goto FLAG;
	cout << "4" << endl;
	cout << "5" << endl;
	cout << "6" << endl;
	FLAG:
	cout << "7" << endl;
	cout << "8" << endl;
	cout << "9" << endl;

	system("pause");
	return 0;
}
